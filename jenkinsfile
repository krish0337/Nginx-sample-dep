pipeline {
  
  agent { 

  stages {

    stage('Checkout Source') {
      steps {
        git url:'https://https://github.com/krish0337/Nginx-sample-dep.git', branch:'main'
      }
    }

    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "dockerfile.yml")
        }
      }
    }

  }

}
